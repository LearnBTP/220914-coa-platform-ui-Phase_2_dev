{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "coa-lineplan-ui-config1",
      "cwd": "${workspaceFolder}/coa-lineplan-ui",
      "runtimeArgs": [
        "fiori",
        "run"
      ],
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npx",
      "args": [
        "--open",
        "index.html"
      ],
      "windows": {
        "runtimeExecutable": "npx.cmd"
      },
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "outputCapture": "std",
      "env": {
        "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/COA-UI/coa-lineplan/coa-lineplan-ui\"}",
        "FIORI_TOOLS_BACKEND_CONFIG": "[{\"path\":\"/sap\",\"url\":\"http://localhost\"}]",
        "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false&responderOn=true"
      }
    },
