<core:FragmentDefinition xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns:table="sap.ui.table"
	xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:smartForm="sap.ui.comp.smartform" 
	xmlns:smartField="sap.ui.comp.smartfield" xmlns:common="sap.suite.ui.commons" xmlns:customselect="com.apple.coa.control">
	<Dialog id="SynchDialog" title="Sync RFID Projection"  contentWidth="30%" class="zDialog" afterClose=".onAfterSyncDilogClose">
		<content>
			<smartForm:SmartForm id="smartSynchForm" editable="true"
				class="sapUiSizeCompact">
				<smartForm:Group id="CoreGroup">
					
					<smartForm:GroupElement id="GH_SITE" label="GH Site">
			 			<MultiComboBox	id="ghSiteInput" loadItems=".getGhSiteDDSync($event,true,'GH_SITE_ORG')" showSelectAll="false"
							items="{
								path: 'UnscannableModel>/GH_Site', templateShareable:false
							}" selectedKeys="{UnscannableModel>/selGHSite}" >
							<core:Item key="{UnscannableModel>GH_SITE_ORG}" text="{UnscannableModel>GH_SITE_ORG}" id="cighsiteinput"/>
							</MultiComboBox>
                    </smartForm:GroupElement>
					<smartForm:GroupElement id="ID_SYN_ALL">
						<CheckBox text="SyncAll" id="idSyncall" select="onSynChkBoxchange"  class="sapUiMediumMarginEnd" enabled="true"  selected="{UnscannableModel>/syncAll}" visible="false"/>
					</smartForm:GroupElement>
				</smartForm:Group>
			</smartForm:SmartForm>
		</content>
		<beginButton>
			<Button xmlns="sap.m" text="Start Sync" id="synOK" press=".onSyncAccept" class="applePrimaryBtn" type="Emphasized"/>
		</beginButton>
		<endButton>
			<Button xmlns="sap.m" text="Cancel" id="btnCancel" press="onSyncDialofClose" class="appleAlternateBtn" />
		</endButton>
	</Dialog>
</core:FragmentDefinition>